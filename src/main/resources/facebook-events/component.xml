        <component name="createMaybeRSVP">
            <file>createMaybeRSVP.xml</file>
            <description>RSVP a user as maybe for an event.</description>
        </component>
        <component name="createPostOnEventWall">
            <file>createPostOnEventWall.xml</file>
            <description>Create a post on the event wall.</description>
        </component>
    </subComponents>
